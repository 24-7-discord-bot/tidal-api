{
  "name": "tidal-api",
  "version": "1.0.0",
  "description": "An unofficial API for Tidal Music",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d lib/ src/",
    "start": "babel-node src/",
    "pretest": "npm run build",
    "test": "mocha --timeout 5000 --require babel-core/register --require babel-polyfill ./test/**/*.js",
    "test:cover": "babel-node ./node_modules/.bin/babel-istanbul cover _mocha -- --timeout 5000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spencercharest/tidal-api.git"
  },
  "keywords": [
    "flac",
    "alac",
    "lossless",
    "music",
    "stream",
    "streaming",
    "tidal"
  ],
  "author": "Spencer Charest",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spencercharest/tidal-api/issues"
  },
  "homepage": "https://github.com/spencercharest/tidal-api#readme",
  "dependencies": {
    "axios": "^0.17.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^4.0.1",
    "nock": "^9.0.27"
  }
}
